<h1>Listing creature_locations</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Creature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creature_locations.each do |creature_location| %>
      <tr>
        <td><%= creature_location.location_id %></td>
        <td><%= creature_location.creature_id %></td>
        <td><%= link_to 'Show', creature_location %></td>
        <td><%= link_to 'Edit', edit_creature_location_path(creature_location) %></td>
        <td><%= link_to 'Destroy', creature_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creature location', new_creature_location_path %>
